<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <display-name>North Wyke Farm Platform SADI Services</display-name>
    <servlet>
        <servlet-name>getCatchment</servlet-name>
        <servlet-class>uk.ac.rothamsted.ide.GetCatchment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getField</servlet-name>
        <servlet-class>uk.ac.rothamsted.ide.GetField</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getAnimalBasicData</servlet-name>
        <servlet-class>uk.ac.rothamsted.ide.GetAnimalBasicData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getDataQuality</servlet-name>
        <servlet-class>uk.ac.rothamsted.ide.GetDataQuality</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getCatchment</servlet-name>
        <url-pattern>/getCatchment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getField</servlet-name>
        <url-pattern>/getField</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getAnimalBasicData</servlet-name>
        <url-pattern>/getAnimalBasicData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getDataQuality</servlet-name>
        <url-pattern>/getDataQuality</url-pattern>
    </servlet-mapping>
</web-app>

